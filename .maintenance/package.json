{
  "name": "maintenance",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "format-self": "prettier --cache --write .",
    "format-check-self": "prettier --cache --check .",
    "clean": "node ./scripts/clean.js",
    "install-local": "node ./scripts/install-local.js",
    "install-remote": "node ./scripts/install-remote.js",
    "update-local": "node ./scripts/update-local.js",
    "update-remote": "node ./scripts/update-remote.js",
    "test": "node ./scripts/test-examples.js",
    "local-registry-check": "node ./scripts/local-registry-check.js",
    "build": "node ./scripts/build.js",
    "format": "node ./scripts/format.js",
    "exec": "node ./scripts/exec.js",
    "outdated": "node ./scripts/outdated.js",
    "audit-local": "node ./scripts/audit-local.js",
    "audit-remote": "node ./scripts/audit-remote.js",
    "all-local": "npm run clean && npm run install-local && npm run update-local && npm run format && npm run build && npm run test && npm run audit-local",
    "all-remote": "npm run clean && npm run install-remote && npm run update-remote && npm run format && npm run build && npm run test && npm run audit-remote"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "zx": "^8.5.0"
  },
  "devDependencies": {
    "prettier": "^3.5.3"
  }
}
